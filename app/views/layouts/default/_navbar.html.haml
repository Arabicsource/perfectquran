%nav.navbar.default{data: {controller: 'navbar'}, role: 'navigation', aria: { label: 'main navigation'} }
  = render 'layouts/default/navbar_brand'

  .navbar-menu{data: {target: 'navbar.menu'}}
    .navbar-end
      - if browser.mobile?
        = render 'layouts/default/navbar_mobile_menu'
      - else
        = render 'layouts/default/navbar_desktop_menu'