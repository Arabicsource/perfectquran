<%= form_with model: @email_preference, method: :patch, id: 'email-preferences-form' do |f| %>
  <div class='field'>
    <div class='control'>
      <%= f.label :general, class: 'checkbox' do %>
        <%= f.check_box :general, id: 'account_email_preferences_general' %>
        General
      <% end %>
    </div>
  </div>
<% end %>

<% provide :title, 'Edit Email Preferences' %>