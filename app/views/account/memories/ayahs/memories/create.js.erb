(function(){
  var progress = document.getElementById('progress');
  var card = document.getElementById('ayah-<%= @ayah.id %>');
  var memorizedColumn = document.getElementById('memorized');

  progress.outerHTML = "<%= j render('account/memories/surahs/partials/progress') %>"
  memorizedColumn.insertAdjacentHTML('beforeend', '<%= j render('account/memories/surahs/partials/memorized_card', ayah: @ayah) %>');
  card.classList.add('is-hidden');
})();
