%table#cities.table.is-fullwidth
  %tbody
    %tr
      %th #
      %th Name
      %th Code
      %th State
      %th Country
  %tbody
    - @cities.each do |city|
      %tr
        %td= city.id
        %td= city.name
        %td= city.code
        %td= city.state_name
        %td= city.country_code