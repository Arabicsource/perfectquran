= form_with url: accounts_card_path, method: :patch, id: 'payment_form' do |f|
  #card-errors

  .field
    #card-element.control

  %p.control
    .buttons
      = f.submit 'Update Payment Method', class: 'button is-orange'
      = link_to 'cancel', accounts_subscription_path, class: 'button is-light'