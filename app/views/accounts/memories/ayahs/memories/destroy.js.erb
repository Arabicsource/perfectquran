(function(){
  var progress = document.getElementById('progress');
  var card = document.getElementById('ayah-<%= @ayah.id %>');
  var notMemorizedColumn = document.getElementById('not-memorized');

  progress.outerHTML = "<%= j render('accounts/memories/surahs/partials/progress') %>"
  notMemorizedColumn.insertAdjacentHTML('beforeend', '<%= j render('accounts/memories/surahs/partials/not_memorized_card', ayah: @ayah) %>');
  card.classList.add('is-hidden');
})();
