= form_with url: accounts_subscription_path, method: :post, id: 'payment_form' do |f|
  #card-errors

  .field
    #card-element.control

  %p.control
    .buttons
      = f.submit 'Subscribe for Just $5 / Month', class: 'button is-orange'
      = link_to 'Cancel', accounts_profile_path, class: 'button is-light'